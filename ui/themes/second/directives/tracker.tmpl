<div id="tracker" ng-show="tracker.length > 0">
    <ul>
        <li ng-repeat="entry in tracker">
        <span ng-if="!entry.current">
            <a href="{{ entry.link }}">{{entry.label}}</a>
            <span class="glyphicon glyphicon-chevron-right"></span>
        </span>
        <span ng-if="entry.current">
            {{entry.label}}
        </span>
        </li>
    </ul>
</div>