<div ng-controller="mainMembersCtrl as mainCtrl">
<tabset id="membersSection">
    <tab heading="Members">
        <section ng-controller="membersCtrl as gridCtrl" class="membersCtrl">
                <span ng-if="access.adminUser.addUser">
                    <a href="" ng-click="addMember()" class="btn btn-primary">Add New Member</a><br/><br/>
                </span>
            <nglist></nglist>
        </section>
    </tab>

    <tab ng-show="access.adminGroup.list" heading="Groups">
        <section ng-controller="groupsCtrl as groupGridCtrl">
                <span ng-if="access.adminGroup.add">
                    <a href="" ng-click="addGroup()" class="btn btn-primary">Add New Group</a><br/><br/>
                </span>
            <nglist></nglist>
        </section>
    </tab>
    <tab heading="Tenants" ng-if="access.adminAll">
        <section ng-controller="tenantsUracCtrl">
            <div class="entryBoxes">
                <div ng-repeat="row in tenantsList" class="entryBox" ng-if="row.locked!==true">
                    <div class="header">
                        {{row['name']}}<span>[ {{row['code']}} ]</span>
                    </div>
                    <div class="body">
                        <tabset id="tenantsMembers">
                            <tab heading="Members">
                                <br/>
                                <manage-members>
                                </manage-members>
                            </tab>
                            <tab heading="Groups">
                                <br/>
                                <manage-groups>
                                </manage-groups>
                            </tab>
                        </tabset>
                    </div>
                </div>
            </div>
        </section>

    </tab>
</tabset>
</div>