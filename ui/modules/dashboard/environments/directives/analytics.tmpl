<section ng-controller="analyticsCtrl">
    <div ng-if="analytics.activated">
        <tabset>
            <tab heading="Web">
                <br />
                <iframe src="http://dashboard.soajs.org:5601/app/kibana#/dashboard/Nginx?embed=true&_g=(refreshInterval:(display:Off,pause:!f,value:0),time:(from:now-15m,mode:quick,to:now))&_a=(filters:!(),options:(darkTheme:!f),panels:!((col:1,id:TimeStamp,panelIndex:1,row:1,size_x:6,size_y:4,type:visualization),(col:7,id:Bytes,panelIndex:2,row:1,size_x:6,size_y:4,type:visualization),(col:1,id:'Top-10-IP!'s',panelIndex:3,row:5,size_x:4,size_y:4,type:visualization),(col:5,id:Agents,panelIndex:4,row:5,size_x:5,size_y:4,type:visualization),(col:6,id:Top-5-Countries,panelIndex:5,row:9,size_x:4,size_y:5,type:visualization),(col:1,id:Map,panelIndex:6,row:9,size_x:5,size_y:5,type:visualization),(col:10,id:Response-Codes,panelIndex:7,row:5,size_x:3,size_y:4,type:visualization),(col:10,id:Top-10-Requests,panelIndex:8,row:9,size_x:3,size_y:5,type:visualization)),query:(query_string:(analyze_wildcard:!t,query:'*')),title:Nginx,uiState:(P-4:(spy:(mode:(fill:!f,name:!n)))))" height="1250px" width="100%"></iframe>
            </tab>
            <tab heading="Cluster">

            </tab>
        </tabset>
    </div>
    <div ng-if="!analytics.activated">
        <alert type="warning" ng-style="{'text-align': 'center'}">
            Analytics has not been activated yet
        </alert>
        <br />
        <button ng-style="{'margin-left': '44%'}" class="btn btn-primary" ng-click="activateAnalytics()">Activate Analytics</button>
    </div>
</section>
