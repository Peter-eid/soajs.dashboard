<section>
    <div class='form' ng-controller="aclCtrl as formCtrl">
        <form novalidate="" class="css-form ng-pristine" name="">
            <div class="header"><h3 class="">Modify ACL of Package: {{currentPackage.code}}</h3></div>
            <div class="body">
                <div id="generate-ApiList" class="entryServiceBoxes">
                    <div ng-repeat="service in allServiceApis" class="clearfix service-container">
                        <div class="clearfix serviceNameHead">
                            <div ng-include="'modules/productization/directives/service-name.tmpl'"></div>
                        </div>
                        <div ng-include="'modules/productization/directives/api-permission.tmpl'"></div>
                    </div>
                </div>
            </div>
            <div class="footer"><br />
                <input type="submit" class="btn btn-sm btn-primary" ng-click="saveACL()" value="Save ACL" />
            </div>
        </form>
    </div>
</section>